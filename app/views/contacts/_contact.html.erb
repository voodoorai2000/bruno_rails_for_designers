
<tr>
    <td><%= contact.name %></td>
    <td><%= contact.email %></td>
    <td><%= contact.newsletter %></td>
    <td><%= contact.sex %></td>
    <td><%= contact.notes %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Unsubscribe', unsubscribe_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
